<% provide(:title, @user.name) %>
<p>
  <strong><i>Name:</i>:</strong>
  <%= @user.name %><%= current_user.name %>
</p>
<p>
  <strong>Email:</strong>
  <%= @user.email %>
</p>

<div class="projectsN">
